!MarsRover methodsFor: 'comands' stamp: 'jeo 5/13/2022 19:17:14'!
processCommand: aComand 
	aComand = '' ifTrue: [^self].
	(1 to: aComand size) do: [:index | 	
		((aComand at: index) = $f) ifTrue: [self moveForward].
		((aComand at: index) = $b) ifTrue: [self moveBackward].
		(aComand at: index) = $l ifTrue: [self rotateLeft].
		(aComand at: index) = $r ifTrue: [self rotateRight]].! !
