		(transaction isKindOf: Deposit) 
			ifTrue: [transaction transfer
				ifNil: [anOrderedCollection add: 'Deposito por ' , transaction value asString , '.']
				ifNotNil: [anOrderedCollection add: 'Entrada por transferencia de ' , transaction value asString , '.']].
				
		(transaction isKindOf: Withdraw) 
			ifTrue: [transaction transfer 
				ifNil: [anOrderedCollection add: 'Extraccion por ' , transaction value asString , '.']
				ifNotNil: [anOrderedCollection add: 'Salida por transferencia de ' , transaction value asString , '.']].		
		].
	
	
	sum := anOrderedCollection inject: '' into: [:string :line | string , line		].
	^ sum.! !
